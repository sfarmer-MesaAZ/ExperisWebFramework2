/* ================================
 * RequireJS Optimizer Build Settings
 * ================================ */
/*
Instructions:
1. Install Node.js <http://nodejs.org/>
2. Open command prompt.
3. cd into the directory containing this file.
4. Run: node lib/r.js -o build.json
*/
{
	"baseUrl": "./",
	"name": "app",
	"include": [
		"lib/require"
	],
	"exclude": [
		"lib/selectivizr-min"
	],
	"optimize": "uglify2",
	"out": "app-built.js",
	"insertRequire": [
		"app"
	],
	"paths": {
		"jquery": "lib/jquery-1.10.2",
		"jquery.sampleplugin": "lib/jquery.sampleplugin",
		"selectivizr": "lib/selectivizr-min"
	},
	"findNestedDependencies": true
}
